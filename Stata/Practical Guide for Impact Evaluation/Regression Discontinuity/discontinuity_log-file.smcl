. local h=2
{txt}
{com}.         while `h'<=10 {c -(}
{txt}  2{com}.         replace norm1=1-abs(sisbenalin/`h')
{txt}  3{com}.         replace k=0 if norm1<0
{txt}  4{com}.         replace k=norm1 if norm1>=0
{txt}  5{com}.         
.         display " Regresión talla para la edad por izquierda. Ancho de banda `h'"
{txt}  6{com}.         reg ha_nchs sisbenalin if  sisben<10 & sisben>=10-`h' [aw=k], robust
{txt}  7{com}.         mat define A_10=e(b)
{txt}  8{com}.         scalar define alfa_`h'l=el(A_10,1,2)
{txt}  9{com}.         mat drop A_10
{txt} 10{com}.         
.         display " Regresión talla para la edad por derecha. Ancho de banda `h'"
{txt} 11{com}.         reg ha_nchs sisbenalin if  sisben>=10 & sisben<=10+`h' [aw=k], robust
{txt} 12{com}.         mat define A_10=e(b)
{txt} 13{com}.         scalar define alfa_`h'r=el(A_10,1,2)
{txt} 14{com}.         mat drop A_10
{txt} 15{com}. 
.         display " Regresión probabilidad participación por izquierda. Ancho de banda `h'"
{txt} 16{com}.         reg D sisbenalin if  sisben<10 & sisben>=10-`h' [aw=k], robust
{txt} 17{com}.         mat define A_10=e(b)
{txt} 18{com}.         scalar define alfa_d`h'l=el(A_10,1,2)
{txt} 19{com}.         mat drop A_10
{txt} 20{com}. 
.         display " Regresión probabilidad participación por derecha. Ancho de banda `h'"
{txt} 21{com}.         reg D sisbenalin if  sisben>=10 & sisben<=10+`h' [aw=k], robust
{txt} 22{com}.         mat define A_10=e(b)
{txt} 23{com}.         scalar define alfa_d`h'r=el(A_10,1,2)
{txt} 24{com}.         mat drop A_10
{txt} 25{com}. 
.         mat IL_KT[`h',1]=(alfa_`h'l-alfa_`h'r)/(alfa_d`h'l-alfa_d`h'r)
{txt} 26{com}. 
. local h=`h'+1
